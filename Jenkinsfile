pipeline {
  agent any
  stages {
    stage('Development Build') {
      steps {
        echo 'Git pull code from repo'
      }
    }

    stage('Smoke Test') {
      steps {
        echo 'Print Smoke test'
      }
    }

    stage('Certify') {
      steps {
        echo 'Certify the build!!'
      }
    }

  }
}